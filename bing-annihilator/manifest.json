{
  "name": "Bing Annihilator",
  "version": "0.01",
  "description": "Annihilates any tab with domain 'bing.com' to 'google.com'",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "background": {
    "scripts": ["main.js"],
    "persistant": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}